#User function Template for python3

class Solution:
	def nthRoot(self, n: int, m: int) -> int:
		# Code here
		for i in range(1,m+1):
		    k = i ** n
		    if k == m:
		        return i
		    if k > m:
		        return -1


#{ 
 # Driver Code Starts
#Initial Template for Python 3

if __name__ == '__main__':
    T = int(input())
    for i in range(T):
        n = int(input())
        m = int(input())
        ob = Solution()
        ans = ob.nthRoot(n, m)
        print(ans)

# } Driver Code Ends
